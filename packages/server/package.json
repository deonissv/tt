{
  "name": "@tt/server",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "NODE_OPTIONS=\"$NODE_OPTIONS NODE_NO_WARNINGS=1 --experimental-modules --experimental-wasm-modules --experimental-require-module --experimental-vm-modules \" NODE_NO_WARNINGS=1  webpack --config ./webpack.config.js --env production",
    "dev": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-modules --experimental-wasm-modules --experimental-require-module --experimental-vm-modules\" NODE_NO_WARNINGS=1  webpack --config ./webpack.config.js --watch",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "vitest run --config ../../vitest.config.ts",
    "test:w": "vitest --config ../../vitest.config.ts",
    "test:e2e": "vitest run --config ../../vitest.e2e.config.ts",
    "typecheck": "tsc --noEmit",
    "start": "node dist/server/src/main.js -e 'node --experimental-require-module'",
    "debug": "nest start --debug --watch -e 'node --experimental-require-module'",
    "db": "prisma generate",
    "db:update": "prisma generate && prisma db push"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@babylonjs/core": "^7.18.0",
    "@babylonjs/havok": "^1.3.7",
    "@babylonjs/loaders": "^7.18.0",
    "@casl/ability": "^6.7.1",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/platform-ws": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/websockets": "^10.3.10",
    "@prisma/client": "^5.17.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv-cli": "^7.4.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "prisma-extension-soft-delete": "1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "unplugin-swc": "^1.5.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "prisma": "^5.17.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "testcontainers": "^10.11.0",
    "vitest": "^2.0.5"
  }
}
