{
  "name": "@tt/server",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-require-module --experimental-vm-modules \" webpack --config webpack.config.js --env production",
    "dev": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-require-module --experimental-vm-modules \" webpack --config webpack.config.js --watch",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "vitest run --config ../../vitest.config.ts",
    "test:w": "vitest --config ../../vitest.config.ts",
    "test:e2e": "vitest run --config ../../vitest.e2e.config.ts",
    "typecheck": "tsc --noEmit",
    "start": "node dist/server/src/main.js -e 'node --experimental-require-module'",
    "debug": "nest start --debug --watch -e 'node --experimental-require-module'",
    "db": "prisma generate",
    "db:update": "prisma generate && prisma db push"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@babylonjs/core": "^7.0.0",
    "@babylonjs/havok": "^1.2.1",
    "@babylonjs/loaders": "^7.0.0",
    "@casl/ability": "^6.5.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-ws": "^10.3.3",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/websockets": "^10.3.3",
    "@prisma/client": "^5.8.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "prisma-extension-soft-delete": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^2.0.12",
    "prisma": "^5.10.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "testcontainers": "^10.4.0",
    "vitest": "^1.6.0"
  }
}
